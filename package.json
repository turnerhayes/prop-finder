    "if-env": "^1.0.4",
    "immutable": "^4.0.0-rc.12",
    "intl": "^1.2.5",
    "morgan": "^1.9.1",
    "mustache": "^3.0.1",
    "primereact": "^3.1.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-react-hook": "^3.2.1",
