  "author": "Turner Hayes <dev@turnerhayes.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --env.NODE_ENV=production --config webpack.config.js --color --progress --hide-modules",
    "build:clean": "rimraf ./dist",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development nodemon server/bin/www --debug",
    "start:prod": "npm run build && cross-env NODE_ENV=production node server/bin/www",
    "start:prod:nobuild": "cross-env NODE_ENV=production node server/bin/www",
    "test": "cross-env NODE_ENV=test jest --watch",
    "test:prod": "cross-env NODE_ENV=test jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
